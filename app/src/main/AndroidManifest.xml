<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="evacorporationlol.happyfellow">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ActivityStart">
            <intent-filter>
                <action android:name="com.evacorporation.action.start"></action>

                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
        <activity android:name=".ActivityRate">
            <intent-filter>
                <action android:name="com.evacorporation.action.rate"></action>

                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
        <activity android:name=".ActivityExit">
            <intent-filter>
                <action android:name="com.evacorporation.actoin.exit"></action>

                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>


    </application>

</manifest>